{
  "name": "virtual-tour-simple",
  "version": "0.1.0",
  "description": "A simple test of showing 360 images for a virtual tour.",
  "main": "public/index.html",
  "type": "module",
  "scripts": {
    "deploy": "run-s deployClient copyToDeploy",
    "copyToDeploy": "node deployScript.js",
    "client": "esbuild --bundle --minify --target=es2020 --define:_DEV_=false --define:_DEPLOY_=false --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/bundle.js client/app.jsx",
    "deployClient": "esbuild --bundle --minify --target=es2020 --define:_DEV_=false --define:_DEPLOY_=true --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/bundle.js client/app.jsx",
    "prodClient": "esbuild --bundle --watch --minify --target=es2020 --define:_DEV_=false --define:_DEPLOY_=false --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/bundle.js client/app.jsx",
    "devClient": "esbuild --bundle --watch --sourcemap --target=es2020 --define:_DEV_=true --define:_DEPLOY_=false --define:process.env.NODE_ENV=\\\"development\\\" --outfile=public/bundle.js client/app.jsx",
    "server": "node server/server.js",
    "devServer": "nodemon --watch server server/server.js dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Seth Berrier",
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@react-spring/three": "^9.5.5",
    "@react-three/drei": "^9.40.0",
    "@react-three/fiber": "^8.9.1",
    "@types/prismjs": "^1.26.0",
    "axios": "^0.27.2",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "howler": "^2.2.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.29.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-hotkeys-hook": "^3.4.4",
    "react-material-ui-carousel": "^3.4.2",
    "react-simple-code-editor": "^0.13.1",
    "recoil": "^0.7.6",
    "subtitles-parser-vtt": "^0.1.0",
    "suspend-react": "^0.0.8",
    "three": "^0.146.0",
    "three-stdlib": "^2.21.8"
  }
}
