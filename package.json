{
  "name": "virtual-tour-simple",
  "version": "0.1.0",
  "description": "A simple test of showing 360 images for a virtual tour.",
  "main": "public/index.html",
  "type": "module",
  "scripts": {
    "server": "node server/server.js",
    "devServer": "nodemon --watch server server/server.js dev",
    "client": "esbuild --bundle --minify --target=es6  --define:_DEV_=false --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/bundle.js client/app.jsx",
    "prodClient": "esbuild --bundle --watch --minify --target=es6 --define:_DEV_=false --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/bundle.js client/app.jsx",
    "devClient": "esbuild --bundle --watch --sourcemap --target=es6 --define:_DEV_=true --define:process.env.NODE_ENV=\\\"development\\\" --outfile=public/bundle.js client/app.jsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Seth Berrier",
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@react-spring/three": "^9.5.2",
    "@react-three/drei": "^9.11.3",
    "@react-three/fiber": "^8.0.19",
    "@react-three/xr": "^3.5.0",
    "axios": "^0.27.2",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "howler": "^2.2.3",
    "nodemon": "^2.0.15",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-hotkeys-hook": "^3.4.4",
    "recoil": "^0.7.6",
    "subtitles-parser-vtt": "^0.1.0",
    "three": "^0.140.0"
  },
  "dependencies": {
    "express": "^4.17.3"
  }
